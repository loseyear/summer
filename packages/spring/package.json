{
  "name": "spring",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "dev": "vite --host --port 9527",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "yarn lint:ts && yarn lint:style && yarn lint:prettier",
    "lint:fix": "yarn lint:ts:fix && yarn lint:style:fix && yarn lint:prettier",
    "lint:ts": "eslint src",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:style": "stylelint src/**/*.ts",
    "lint:style:fix": "yarn lint:style --fix",
    "lint:prettier": "prettier --write 'src/**/*.{ts, tsx}'"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@uiw/react-baidu-map": "^2.6.0",
    "axios": "^1.4.0",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "styled-components": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/axios": "^0.14.0",
    "@types/bmapgl": "^0.0.7",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-react": "^4.0.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "path": "^0.12.7",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "terser": "^5.16.9",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.3.0"
  }
}
